<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.vertineko.shenpi.dao.FileDao">
    <insert id="addFile">
        insert into `exfile`(`id`,`file`,`type`) values (#{id},#{file},#{type})
    </insert>
    <select id="getfile">
        select * from `exfile` where id = #{id}
    </select>
</mapper>